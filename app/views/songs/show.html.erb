<div>
  <div class="col-md-6">
    <img src="<%= @song.artist.artist_image_path%>">
    <br>
    <h1><%= @song.title %></h1>
    <p> by </p>
    <%= link_to @song.artist.artist_name, artist_path(@song.artist) %>
    <br>
  </div>
  <div class="col-md-6">
    <iframe width="560" float="left" height="315" src="<%= @song.youtube.gsub('watch?v=', 'embed/') %>" frameborder="0" allowfullscreen></iframe>
  </div>
</div>
<br>
<h1> appears on playlists </h1>
<% @song.playlists.each do |p| %>
  <%= link_to p.name, playlist_path(p) %>
  <br>
<% end %>
<br>
<%= link_to "Edit", edit_song_path(@song) %> 
<span class="glyphicon glyphicon-minus"></span>
<%= link_to 'Delete', song_path(@song), :method => :delete %>
<br>
